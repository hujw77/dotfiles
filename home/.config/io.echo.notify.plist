<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
"http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>io.echo.notify</string>
    <key>KeepAlive</key>
    <true/>
    <key>StandardErrorPath</key>
    <string>/Users/hujingwei/.notify-stderr.log</string>
    <key>StandardOutPath</key>
    <string>/Users/hujingwei/.notify-stdout.log</string>
    <key>ProgramArguments</key>
    <array>
      <string>/opt/homebrew/bin/notify</string>
       <string>/opt/homebrew/bin/fish</string>
       <string>-c</string>
       <string>change_background</string>
    </array>
</dict>
</plist>
