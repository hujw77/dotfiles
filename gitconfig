[alias]
	st = status -sb
	ci = commit
	reci = commit --amend
	all = commit -am
	br = branch
	co = checkout
	di = diff
	dic = diff --cached
	lg = log -p
	lol = log --graph --decorate --pretty=oneline --abbrev-commit
	lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
	ls = ls-files
	# Show files ignored by git:
	ign = ls-files -o -i --exclude-standard

	shoot = push origin --delete
	unstage = reset HEAD --
	prev = checkout -
	discard = checkout --

[color]
	ui = auto
[color "grep"]
	linenumber = yellow
	match = red
	filename = magenta
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
